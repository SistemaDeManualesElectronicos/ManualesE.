@model ManualesElectronicosFInalFinal2.Models.DocentesViewModels.SubtemasViewModel

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <script src="https://cdn.tiny.cloud/1/000kyaippw4hcazl7rr5odphr0eguildpbtxjvfkov7w6i9j/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>
</head>
<body>
    <textarea>
    Welcome to TinyMCE!
  </textarea>
    <script>
        tinymce.init({
            selector: 'textarea',
            plugins: 'a11ychecker advcode casechange formatpainter linkchecker autolink lists checklist media mediaembed pageembed permanentpen powerpaste table advtable tinycomments tinymcespellchecker',
            toolbar: 'a11ycheck addcomment showcomments casechange checklist code formatpainter pageembed permanentpen table',
            toolbar_mode: 'floating',
            tinycomments_mode: 'embedded',
            tinycomments_author: 'Author name',
        });
    </script>

    <input type="button" value="Agregar"/>
    <input type="button" value="cancelar"/>
</body>
</html>

